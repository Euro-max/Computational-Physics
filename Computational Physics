        "from sympy import symbols, integrate\n",
        "\n",
        "# Define variables\n",
        "x, y,z= symbols('x y z')\n",
        "\n",
        "# Define the integrand (flux integrand expression)\n",
        "flux_integrand = (y+1)\n",
        "from sympy import symbols, integrate\n",
        "\n",
        "# Define variables\n",
        "x, y, z = symbols('x y z')\n",
        "\n",
        "# Define the integrand (flux integrand expression)\n",
        "flux_integrand = (y+1)\n",
        "from sympy import symbols, integrate\n",
        "\n",
        "# Define variables\n",
        "x, y, z = symbols('x y z')\n",
        "\n",
        "# Define the integrand (flux integrand expression)\n",
        "flux_integrand = (y+1)\n",
        "\n",
        "# Set the bounds for the region D (0 <= x <= 3, 0 <= y <= 3 - x)\n",
        "flux_integral = integrate(integrate(integrate(flux_integrand,(z,0,6-2*x-2*y)),(y, 0, 3 - x)), (x, 0, 3))\n",
        "\n",
        "# Evaluate the integral\n",
        "flux_integral.evalf()\n",
        "flux_integral.evalf()\n",
        "flux_integral.evalf()\n"
