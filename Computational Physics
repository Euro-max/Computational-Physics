        "##Integrate a function given parameters\n",
        "import sympy as smp\n",
        "import numpy as np\n",
        "import matplotlib.pyplot as plt\n",
        "from sympy import Array\n",
        "from scipy.integrate import quad_vec\n",
        "a=np.linspace(0,7,20)\n",
        "b=np.linspace(2,0,1)\n",
        "def f(x,a,b):\n",
        "  return np.sin(x-a*np.pi**(0.5)*np.exp(-b*x))\n",
        "integral=quad_vec(f,0,smp.pi/2,args=(a,b))[0]\n",
        "plt.plot(a,integral,label='Value of Integral vs a')\n",
        "\n",
        "plt.legend()\n",
        "plt.grid()\n",
        "plt.show()"
