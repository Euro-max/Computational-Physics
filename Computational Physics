        "\n",
        "A = [[2,-6,9,-1,8,2], [2,-6,9,-1,9,15], [-1,3,-4,2,-5,4],[3,-9,13,-3,14,11]]\n",
        "\n",
        "A = Matrix(A)\n",
        "\n",
        "# Number of Columns\n",
        "NoC = A.shape[1]\n",
        "\n",
        "# Rank of A\n",
        "rank = A.rank()\n",
        "nullity = NoC - rank\n",
        "\n",
        "print(\"Nullity : \", nullity)\n",
        "print(rank)\n",
