    {
      "cell_type": "code",
      "source": [
        "f,g=smp.symbols('f g',cls=smp.Function)\n",
        "y=smp.symbols('y')\n",
        "g=g(x)\n",
        "f=f(x*y+g)\n",
        "smp.diff(f,x)\n"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/",
          "height": 61
        },
        "id": "jRvrbbh6ypMA",
        "outputId": "c963d548-08b2-4728-c3a7-be85a92adfd6"
      },
      "execution_count": null,
      "outputs": [
        {
          "output_type": "execute_result",
          "data": {
            "text/plain": [
              "(y + Derivative(g(x), x))*Subs(Derivative(f(_xi_1), _xi_1), _xi_1, x*y + g(x))"
            ],
            "text/latex": "$\\displaystyle \\left(y + \\frac{d}{d x} g{\\left(x \\right)}\\right) \\left. \\frac{d}{d \\xi_{1}} f{\\left(\\xi_{1} \\right)} \\right|_{\\substack{ \\xi_{1}=x y + g{\\left(x \\right)} }}$"
