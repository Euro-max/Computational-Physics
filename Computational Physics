    {
      "cell_type": "code",
      "source": [
        "import numpy as np\n",
        "import sympy as smp\n",
        "import matplotlib.pyplot as plt\n",
        "from sympy import symbols\n",
        "\n",
        "t = smp.symbols('t')  # Define t as a symbolic variable\n",
        "v = smp.symbols('v')\n",
        "y = -0.5 * 9.81 * t**2 + v * t\n",
        "\n",
        "v_val = 10  # Value for v\n",
        "y_val = y.subs({v: v_val})\n",
        "v_2=15\n",
        "y_1=y.subs({v:v_2})\n",
        "y_2=[]  # Substitute v with its value\n",
        "\n",
        "t_num = np.linspace(0, 2, 50)  # Numerical values for t\n",
        "\n",
        "y_num = []\n",
        "for time in t_num:\n",
        "    y_num.append(y_val.subs({t: time}))  # Substitute t with numerical values\n",
        "for time in t_num:\n",
        "    y_2.append(y_1.subs({t: time}))\n",
        "\n",
        "plt.plot(t_num, y_num,label='10 m/s')\n",
        "plt.plot(t_num,y_2,label='15m/s')\n",
        "plt.xlabel('Time (s)')\n",
        "plt.ylabel('y(t)')\n",
        "plt.grid()\n",
        "plt.legend()"
