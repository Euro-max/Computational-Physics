        "##Simpson's Rule\n",
        "import sympy as smp\n",
        "k,y=smp.symbols('k y')\n",
        "def g(y):\n",
        "  return y\n",
        "N=1000\n",
        "b=10\n",
        "a=9\n",
        "h=(b-a)/N\n",
        "q=0.3*g(a)+0.33*g(b)\n",
        "for o in range(1,N/2):\n",
        "    q=q+1.333*g(a+(2*o-1)*h)+0.67*g(a+2*(o-1)*h)\n",
