    {
      "cell_type": "code",
      "source": [
        "import numpy as np\n",
        "\n",
        "def catalan_generator(max_number):\n",
        "    n = 0\n",
        "    C_n = 1\n",
        "    C_list = [1.0]\n",
        "    while C_n < max_number:\n",
        "        ##Recursive\n",
        "        C_n = ( (4*n + 2) / (n + 2) ) * C_n\n",
        "        n += 1\n",
        "        C_list.append(C_n)\n",
        "    return C_list\n",
        "\n",
        "print(catalan_generator(float(input(\"Up to what number should I calculate Catalan Numbers?\"))))"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
