      "cell_type": "code",
      "source": [
        "a = 1.0\n",
        "eps = 0.1\n",
        "while a + eps != a:\n",
        "    eps /= 2.0\n",
        "\n",
        "# note: eps is now the first value such that 1 + eps is not equal to 1\n",
        "# so 2*eps is the last value for which they were distinct\n",
        "print(2*eps)"
      ],
      "metadata": {
